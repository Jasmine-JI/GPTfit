{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev-ssl-start": "ng serve -c dev-web --ssl --ssl-cert /etc/ssl/free.crt --ssl-key /etc/ssl/free.key --port 8080 --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json",
    "web-start": "ng serve -c dev-web --port 8080 --host 192.168.1.235 --proxy-config proxy.conf.json",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --port 8080 --host 0.0.0.0 --hmr --disable-host-check --proxy-config proxy.conf.json",
    "pm2-start": "pm2 start -i 4 server.js --log-date-format 'YYYY-DD-MM HH:mm:ss.SSS'",
    "pm2-delete": "pm2 delete all",
    "pm2-kill": "pm2 kill",
    "bash": "./reset.sh",
    "build": "ng build",
    "preweb-build": "",
    "pretst-build": "npm run pm2-kill && find dist/* -type l -delete",
    "preuat-build": "npm run pm2-kill && cp src/environments/robots.uat.txt src/robots.txt && find dist/* -type l -delete",
    "preprod-build": "npm run pm2-kill && find dist/* -type l -delete",
    "postweb-build": "npm run bash",
    "posttst-build": "npm run bash",
    "postuat-build": "npm run bash",
    "postprod-build": "npm run bash",
    "web-build": "ng build -c web --aot --output-hashing=all --base-href /",
    "uat-build": "ng build -c uat --aot --output-hashing=all --base-href /",
    "tst-build": "ng build -c tst --aot --base-href /",
    "prod-build": "ng build --configuration production --aot --build-optimizer --output-hashing=all --base-href /",
    "patch": "cp patch/regression-simple-linear.d.ts node_modules/ml-regression-simple-linear/regression-simple-linear.d.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^13.2.2",
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
