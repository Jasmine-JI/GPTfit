{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "web": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/robots.txt",
              "src/sitemap.xml"
            ],
            "styles": [
              "src/styles/icon.scss",
              "node_modules/leaflet/dist/leaflet.css",
              "src/styles/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/daterangepicker/daterangepicker.css",
              "src/styles/theme/light.scss",
              "src/styles/theme/dark.scss",
              "src/styles/module/info.scss",
              "src/styles/module/report.scss",
              "src/styles/module/dialog.scss",
              "src/styles/module/operation-report.scss",
              "src/styles/module/official-activity.scss",
              "src/styles/module/app-webview.scss",
              "src/styles/module/grid.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/model"
              ]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.min.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet-omnivore/leaflet-omnivore.min.js",
              "./node_modules/heatmap.js/build/heatmap.js",
              "./node_modules/heatmap.js/plugins/leaflet-heatmap/leaflet-heatmap.js",
              "node_modules/daterangepicker/daterangepicker.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "uat": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.uat.ts"
                }
              ],
              "index": {
                "input": "src/environments/index.uat.html",
                "output": "index.html"
              }
            },
            "tst": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.tst.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "web": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.web.ts"
                }
              ],
              "index": {
                "input": "src/environments/index.web.html",
                "output": "index.html"
              }
            },
            "dev-web": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.web.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web:build"
          },
          "configurations": {
            "uat": {
              "browserTarget": "web:build:uat"
            },
            "tst": {
              "browserTarget": "web:build:tst"
            },
            "production": {
              "browserTarget": "web:build:production"
            },
            "web": {
              "browserTarget": "web:build:web"
            },
            "dev-web": {
              "browserTarget": "web:build:dev-web"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet-omnivore/leaflet-omnivore.min.js"
            ],
            "styles": [
              "src/styles/icon.scss",
              "node_modules/leaflet/dist/leaflet.css",
              "src/styles/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/daterangepicker/daterangepicker.css",
              "src/styles/theme/light.scss",
              "src/styles/theme/dark.scss",
              "src/styles/module/info.scss",
              "src/styles/module/report.scss",
              "src/styles/module/dialog.scss",
              "src/styles/module/official-activity.scss",
              "src/styles/module/app-webview.scss",
              "src/styles/module/grid.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/model"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "sitemap.xml",
              "robots.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "web-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "web:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics",
      "@angular-eslint/schematics"
    ]
  }
}
