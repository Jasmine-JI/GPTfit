{
  "compileOnSave": false,
  "compilerOptions": {
    // "typescript.tsdk": "../node_modules/typescript/lib",
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "app/shared/*"
      ]
      // "echarts": ["node_modules/echarts/dist/echarts.min.js"]
    },
    "outDir": "./dist/out-tsc",
